/* NAppGUI forms */

/* clang-format off */

#if defined(NAPPGUI_SHARED)
    #if defined(NAPPGUI_BUILD_NAPFORMS_LIB)
        #define NAPPGUI_NAPFORMS_EXPORT_DLL
    #else
        #define NAPPGUI_NAPFORMS_IMPORT_DLL
    #endif
#endif

#if defined(__GNUC__)
    #if defined(NAPPGUI_NAPFORMS_EXPORT_DLL)
        #define _napforms_api __attribute__((visibility("default")))
    #else
        #define _napforms_api
    #endif
#elif defined(_MSC_VER)
    #if defined(NAPPGUI_NAPFORMS_IMPORT_DLL)
        #define _napforms_api __declspec(dllimport)
    #elif defined(NAPPGUI_NAPFORMS_EXPORT_DLL)
        #define _napforms_api __declspec(dllexport)
    #else
        #define _napforms_api
    #endif
#else
    #error Unknown compiler
#endif

/* clang-format on */
