/* NForm library import/export */

/* clang-format off */

#if defined(NAPPGUI_SHARED)
    #if defined(NAPPGUI_BUILD_NFORM_LIB)
        #define NAPPGUI_NFORM_EXPORT_DLL
    #else
        #define NAPPGUI_NFORM_IMPORT_DLL
    #endif
#endif

#if defined(__GNUC__)
    #if defined(NAPPGUI_NFORM_EXPORT_DLL)
        #define _nform_api __attribute__((visibility("default")))
    #else
        #define _nform_api
    #endif
#elif defined(_MSC_VER)
    #if defined(NAPPGUI_NFORM_IMPORT_DLL)
        #define _nform_api __declspec(dllimport)
    #elif defined(NAPPGUI_NFORM_EXPORT_DLL)
        #define _nform_api __declspec(dllexport)
    #else
        #define _nform_api
    #endif
#else
    #error Unknown compiler
#endif

/* clang-format on */
